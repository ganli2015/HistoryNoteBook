<Window x:Class="HistoryNoteBook.TagSelect"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="选择标签" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="210*" />
            <RowDefinition Height="51*" />
        </Grid.RowDefinitions>
        <Button Content="确定" Height="23" HorizontalAlignment="Left" Margin="87,16,0,0" Name="button_OK" VerticalAlignment="Top" Width="75" Grid.Row="1" Click="button_OK_Click" />
        <Button Content="取消" Height="23" HorizontalAlignment="Right" Margin="0,16,12,0" Name="button_Cancel" VerticalAlignment="Top" Width="75" Grid.Row="1" Click="button_Cancel_Click" />
        <StackPanel Height="210" HorizontalAlignment="Left" Name="stackPanel1" VerticalAlignment="Top" Width="278" />
    </Grid>
</Window>
